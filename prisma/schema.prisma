generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./cweather.db"
}

model byMinute {
  id        Int     @id @default(autoincrement())
  placeId  String
  temperature Float
  humidity Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model byHour {
  id        Int     @id @default(autoincrement())
  placeId  String
  temperature Float
  humidity Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model byDay {
  id        Int     @id @default(autoincrement())
  placeId  String
  temperature Float
  humidity Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model byMonth {
  id        Int     @id @default(autoincrement())
  placeId  String
  temperature Float
  humidity Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model byYear {
  id        Int     @id @default(autoincrement())
  placeId  String
  temperature Float
  humidity Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

